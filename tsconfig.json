{
    "compilerOptions": {
        "target": "ES2019",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "src",
        "paths": {
            "@data_structures/*": ["data_structures/*"],
            "@errors/*": ["errors/*"]
        },
    }
}
